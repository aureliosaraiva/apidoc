<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
      <title>DigitalOcean API</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=1200, maximum-scale=1" />
    <link rel="alternate" type="application/rss+xml" title="API Changelog | DigitalOcean" href="/changelog/feed.atom"/>

    <link href="assets/css/stylus.css" rel="stylesheet" type="text/css" />
          

    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/styles/default.min.css">
    




  </head>
  <body class="index">

      <header>
  <a href="https://www.digitalocean.com" class="logo"></a>
</header>
 

    
    <a href="#" class="navicon"></a>
<nav class="sidebar">
  <ul id="spyMe" class="nav">
    {{ _.forEach(apidoc, function(item) { }}
    <li>
      <a href="#{{= item.id }}">{{= item.name }}</a>
      <ul class="nav">
        <li><a href="#{{= item.intro.id }}">{{= item.intro.name }}</a></li>

        {{ _.forEach(item.section, function(item2) { }}
        <li><a href="#{{= item2.id }}">{{= item2.name }}</a></li>
        {{ }); }}
      </ul>
    </li>
    {{ }); }}

    
  </ul>
</nav>

  <div id="big-wrap">
    <div id="inner-wrap">

{{ _.forEach(apidoc, function(item) { }}
    
    <div class="set category-head">
      <div class="inner-set" id="{{= item.intro.id }}">
        <div class="set-description">{{= item.intro.content }}</div>
            <div class="set-curl">
                {{= item.intro.code}}
              
            </div>  
      </div>
    </div>

    {{ _.forEach(item.section, function(item2) { }}
        <!-- New Section : Intro -->
        <div class="set">
          <div class="inner-set" id="{{= item2.id }}">
            <div class="set-description">{{= item2.content }}</div>
                <div class="set-curl">
                  {{= item2.code}}
                </div>
          </div> 
        </div>
    {{ }); }}
{{ }); }}




    </div>
  </div>
  <script src="assets/js/all.js" type="text/javascript"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/highlight.min.js"></script>


    <script type="text/javascript">
      $(document).ready(function(){
        var $document = $(document),
        headerHeight = $("header").height();

        if ($document.scrollTop() >= headerHeight) {
          $(".sidebar").addClass('fixed');
        } else {
          $(".sidebar").removeClass('fixed');
        }


        if ($document.scrollTop() >= 90) {
          $(".navicon").addClass('scroll');
        } else { 
          $(".navicon").removeClass('scroll');
        }

        $document.scroll(function() {
          if ($document.scrollTop() >= headerHeight) {
            $(".sidebar").addClass('fixed');
          } else {
            $(".sidebar").removeClass('fixed');
          }

          if ($document.scrollTop() >= 90) {
            $(".navicon").addClass('scroll');
          } else {
            $(".navicon").removeClass('scroll');
          }
        });


        $('pre code').each(function(i, block) {
          hljs.highlightBlock(block);
        }); 

        function slideNav(){
          $(".navicon").toggleClass("show");
          $(".set-description").toggleClass("open");
          $("#big-wrap").toggleClass("open");
        }

        $(window).load(function(){
           $('body').scrollspy({
              target: '#spyMe',
              offset: 20
            });
        });

        $(window).resize(function() {
          $('body').scrollspy('refresh');
        });
      });
    </script>



      


  </body>
</html>
